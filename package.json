{
	"name": "open-in-ipython",
	"publisher": "voilalex",
	"repository": {
		"type": "git",
		"url": "https://github.com/VoIlAlex/vscode-open-in-ipython"
	},
	"icon": "data/icon.png",
	"displayName": "Open in IPython",
	"description": "Using this extension you can open a file with IPython right from File Explorer context menu.",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.openInIPython"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [{
			"command": "extension.openInIPython",
			"title": "Open in IPython"
		}],
		"menus": {
			"explorer/context": [{
				"command": "extension.openInIPython",
				"group": "YourGroup@1"
			}]
		}
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"eslint": "^6.6.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	}
}